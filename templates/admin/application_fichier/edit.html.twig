{% extends 'admin/base.html.twig' %}
{% import "macros/link.html.twig" as link %}
{% block title %}Edit ApplicationFichier
{% endblock %}

{% block body %}
	<div class="card card-outline card-primary">
		<div class="card-header">
			<div class="card-title">Modifier un fichier</div>
		</div>
		<div class="card-body">
			<h3>
				Application:
				{{ application_fichier.application.nom}}</h3>
			{{ include('admin/application_fichier/_form.html.twig', {'button_label': 'Update'}) }}
			{{ include('admin/application_fichier/_delete_form.html.twig') }}
		</div>
	</div>

	{{ link.back(path('app_admin_application_edit',{id:application_fichier.application.id})) }}

{% endblock %}
