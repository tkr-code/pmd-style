{% extends 'admin/base.html.twig' %}
{% import "macros/breadcrumb.html.twig" as breadcrumbs %}
{% import "macros/alertes.html.twig" as alerte %}
{% import "macros/link.html.twig" as links %}

{% block title %}New Caisse{% endblock %}
{% block breadcrumb %}
	{{ breadcrumbs.breadcrumb('Caisse',[
{
path:path('admin_caisse_index'),
name:'Manage Caisse'|trans
}
])}}
{% endblock %}
{% block body %}
{% for item in app.flashes('success') %}
{{ alerte.alert(item,'success')}}
{% endfor %}
<div class="card card-outline card-primary">
    <div class="card-header">
        <h3>Create new Caisse</h3>
    </div>
    <div class="card-body">
        {{ include('admin/caisse/_form.html.twig') }}
        {{ links.back(path('admin_caisse_index')) }}
    </div>
</div>
{% endblock %}
